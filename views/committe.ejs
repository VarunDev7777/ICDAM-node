        <div class="technical_committee_container comm_container vanish">
            <h1 class="section_heading">Technical Committee</h1>
            <div class="main_techp_con">
                <% for(let i=0; i<techMemList.length; i++) { %>
                    <div class="techp_item">
                        <div><strong><%= techMemList[i].name %></strong><span>:</span><span><%= techMemList[i].information %></span></div>
                    </div>
