<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="ScreenOrientation" content="autoRotate:disabled">
    <title>ICDAM | International Conference on Data Analytics and Management</title>
    <link rel="stylesheet" href="/assets/css/committe.css">
    <link rel="stylesheet" href="/assets/css/common.css">
    <link rel="stylesheet" href="/assets/css/header.css">
    <link rel="stylesheet" href="/assets/css/footer.css">
    <link rel="stylesheet" href="/assets/css/sidebar.css">
    <link rel="icon" href="/assets/images/gurunanak.png">
</head>
<body>
    <%- include("./Components/loader.ejs") %>
    <%- include("./Components/header.ejs") %>
    <%- include("./Components/sidebar.ejs") %>
    <div class="committee_container">
        <div class="committee_page_options" data-active='0' tabindex="0">
            <span class="active_option">Steering Committee</span>
            <div class="coption_container vanish">
                <div class="coption cactive">Steering Committee</div>
                <div class="coption">Technical Program Committee</div>
                <div class="coption">Advisory Committee</div>
                <div class="coption">European Technical Board</div>
            </div>
        </div>

        <div class="steering_committee_container comm_container">
            <h1 class="section_heading">Steering Committee Members</h1>
            <div class="main_stcom_section">
                <h1>Patron(s)::</h1>
                <div class="patron_section com_section">
                    <% for(let i=0; i<steerMemList.length; i++) { %>
                        <% if(steerMemList[i].steercompost_id === 1) { %>
                            <div class="comlist_item">
                                <div class="profile_img"><img src="/assets/images/svgs/person-circle-outline.svg"></div>
                                <div class="comitem_info">
                                    <span><%= steerMemList[i].name %></span>
                                    <span>(<%= steerMemList[i].information %>)</span>
                                </div>
                            </div>
                    <% } } %>
                </div>
                <h1>General Chair(s)::</h1>
                <div class="general_chair_section com_section">
                    <% for(let i=0; i<steerMemList.length; i++) { %>
                        <% if(steerMemList[i].steercompost_id === 2) { %>
                            <div class="comlist_item">
                                <div class="profile_img"><img src="/assets/images/svgs/person-circle-outline.svg"></div>
                                <div class="comitem_info">
                                    <span><%= steerMemList[i].name %></span>
                                    <span>(<%= steerMemList[i].information %>)</span>
                                </div>
                            </div>
                    <% } } %>
                </div>
                <h1>Honorary Chair(s)::</h1>
                <div class="honorary_chair_section com_section">
                    <% for(let i=0; i<steerMemList.length; i++) { %>
                        <% if(steerMemList[i].steercompost_id === 3) { %>
                            <div class="comlist_item">
                                <div class="profile_img"><img src="/assets/images/svgs/person-circle-outline.svg"></div>
                                <div class="comitem_info">
                                    <span><%= steerMemList[i].name %></span>
                                    <span>(<%= steerMemList[i].information %>)</span>
                                </div>
                            </div>
                    <% } } %>
                </div>
                <h1>Conference Chair(s)::</h1>
                <div class="conference_chair_section com_section">
                    <% for(let i=0; i<steerMemList.length; i++) { %>
                        <% if(steerMemList[i].steercompost_id === 4) { %>
